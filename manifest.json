{
  "name": "DDG Bangs",
  "description": "Use DuckDuckGo Bangs with Google.com or Bing.com as your default search engine.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [ "activeTab", "webNavigation" ],
  "icons": {
    "16": "/images/ddg-bangs@16.png",
    "32": "/images/ddg-bangs@32.png",
    "128": "/images/ddg-bangs@128.png"
  }
}
